	email = victor.souza@vmlbrasil.com
[github]
	user = victormagalhaes
[alias]
	br = branch
	ci = commit -m
	co = checkout
	df = diff --color --color-words --abbrev
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
	lp = log -p
	st = status
	diffdir = =
[core]
	editor = atom --wait
	excludesfile = ~/.gitignore
[color]
	ui = auto
[color "branch"]
	current = yellow
	local = cyan bold
	remote = green bold
[color "diff"]
	frag = magenta bold
	meta = yellow bold
	new = green bold
	old = red bold
[color "status"]
	added = yellow bold
	changed = green bold
	untracked = cyan bold
[push]
	default = simple
[pull]
 rebase = true
